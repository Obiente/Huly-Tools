{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@composables/*": ["composables/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"]
    },
    "jsx": "preserve"
  },
}
